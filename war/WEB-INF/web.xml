<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xmlns="http://java.sun.com/xml/ns/javaee"
xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
	<servlet>
		<servlet-name>Pair</servlet-name>
		<servlet-class>model.PairServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Pair</servlet-name>
		<url-pattern>/pair</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>Reservation</servlet-name>
		<servlet-class>jp.servlet.ReservationMain</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Reservation</servlet-name>
		<url-pattern>/reservation</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>ReservationCheck</servlet-name>
		<servlet-class>jp.servlet.StaffReservationCheck</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>ReservationCheck</servlet-name>
		<url-pattern>/reservationcheck</url-pattern>
	</servlet-mapping>
	<welcome-file-list>
		<welcome-file>index.jsp</welcome-file>
	</welcome-file-list>
</web-app>
